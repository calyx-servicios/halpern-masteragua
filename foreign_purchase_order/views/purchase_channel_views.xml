<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="purchase_channel_form" model="ir.ui.view">
        <field name="model">purchase.channel</field>
        <field name="arch" type="xml">
            <form string="Type">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="purchase_channel_list" model="ir.ui.view">
        <field name="name">purchase.channel.list</field>
        <field name="model">purchase.channel</field>
        <field name="arch" type="xml">
            <tree string="Channels">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="purchase_channel_search" model="ir.ui.view">
        <field name="name">purchase.channel.search</field>
        <field name="model">purchase.channel</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
            </search>
        </field>
    </record>
</odoo>