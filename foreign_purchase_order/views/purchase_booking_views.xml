<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="purchase_booking_form" model="ir.ui.view">
        <field name="name">purchase.booking.form</field>
        <field name="model">purchase.booking</field>
        <field name="arch" type="xml">
            <form string="Type">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="purchase_booking_list" model="ir.ui.view">
        <field name="name">purchase.booking.list</field>
        <field name="model">purchase.booking</field>
        <field name="arch" type="xml">
            <tree string="Transportations Means">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="purchase_booking_search" model="ir.ui.view">
        <field name="name">purchase.booking.search</field>
        <field name="model">purchase.booking</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
            </search>
        </field>
    </record>
</odoo>